public class problem01 {
    public static void main(String[] args) {

        System.out.println("[구구단 출력]");
        for (int i=1; i<10; i++) {
            for (int j=1; j < 10; j++) {
                System.out.printf(String.format("%d X %d = %d  ",j,i,j*i));
            }
            System.out.printf("\n");
        }
    }
}
